          <colspec colnum="15" colname="c15"/>
          <tbody>
            <row>
              <entry namest="c1" nameend="c15">Beskrywende Woorde</entry>
            </row>
            <row>
              <entry namest="c1" nameend="c2">rus</entry>
              <entry namest="c3" nameend="c4">skaduwee</entry>
              <entry namest="c5" nameend="c6">dun</entry>
              <entry namest="c7" nameend="c9">donker</entry>
