              <entry namest="c14" nameend="c15">groot</entry>
            </row>
            <row>
              <entry>plat</entry>
              <entry namest="c2" nameend="c3">hard</entry>
              <entry namest="c4" nameend="c5">sterk</entry>
              <entry namest="c6" nameend="c7">lig</entry>
              <entry>sag</entry>
              <entry namest="c9" nameend="c10">fyn</entry>
              <entry namest="c11" nameend="c12">hoekig</entry>
