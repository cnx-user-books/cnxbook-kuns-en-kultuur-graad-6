            <tbody>
              <row>
                <entry>Frase 1</entry>
                <entry>Voortstuwend</entry>
                <entry/>
                <entry>Minder beweging en sinkopasie</entry>
                <entry/>
                <entry>Herhaling van vorige frase</entry>
                <entry/>
              </row>
              <row>
                <entry>Frase 2</entry>
                <entry>Voortstuwend</entry>
                <entry/>
                <entry>Minder beweging en sinkopasie</entry>
                <entry/>
                <entry>Herhaling van vorige frase</entry>
                <entry/>
              </row>
              <row>
                <entry>Frase 3</entry>
                <entry>Voortstuwend</entry>
